#include<iostream>
#include<cstring>
using namespace std;
int main(){
char ur[100];
int chislo=0,Bukv=0,bukv=0,error=0,mb=0,mB=0,output=0;
cin>>ur;
//cout<<strlen(ur);
for (int i=0;i<strlen(ur);i++){
    if(ur[i]=='a' or ur[i]=='e' or ur[i]=='y' or ur[i]=='u' or ur[i]=='i' or ur[i]=='o'){Bukv++;bukv=0;}
    else {bukv++;Bukv=0;};
    if(Bukv==3){output++;Bukv=1;}
    if(bukv==3){output++;bukv=1;}
    //if(mb==3){mb=1;output++;}
    //if(mB==3){mB=0;output++;}
    }
cout<<output<<endl;
}
