#include <iostream>
#include <cstring>
using namespace std;
int main(){
    char ur[200]={0};
    long long int check=0;
    cin>>ur;
    //cout<<strlen(ur)<<endl;
    for(int i=0;i<strlen(ur);i++){
    if(ur[i]=='6' or ur[i]=='9' or ur[i]=='0'){
    check++;
    }
    else if (ur[i]=='8'){
    check=check+2;
    }

    }
    cout<<check;
}
